{
    "name": "react-new",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "babel-preset-stage-0": "^6.24.1",
        "classnames": "^2.2.6",
        "jest-enzyme": "^7.0.2",
        "node-sass": "^4.11.0",
        "react": "^16.8.5",
        "react-dom": "^16.8.5",
        "react-scripts": "2.1.8"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom --verbose --coverage",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@storybook/addon-actions": "^5.0.5",
        "@storybook/addon-links": "^5.0.5",
        "@storybook/react": "^5.0.5",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/stories/**/*.{js,jsx}",
            "!src/index.js",
            "!src/serviceWorker.js",
            "!src/setupTests.js"
        ],
        "coverageReporters": [
            "lcov",
            "html",
            "json"
        ]
    }
}
